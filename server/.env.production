# Environment variables for production
# Do not store actual secrets here - these should be set in Render's environment variables

# Database - this will be set with the actual Render PostgreSQL connection string
DATABASE_URL="YOUR_RENDER_POSTGRES_INTERNAL_URL"

# JWT Configuration
JWT_SECRET="STRONG_RANDOM_SECRET_SET_IN_RENDER"
JWT_EXPIRES_IN="1d"

# Server Configuration
PORT=10000
NODE_ENV="production"

# Client URLs (for CORS)
CLIENT_URL="https://myvetstudyapp.netlify.app" 